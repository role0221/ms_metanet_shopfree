{
  "compilerOptions": {
    "noUnusedLocals": true,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@/utils/*": [
        "utils/*"
      ],
      "@/config/*": [
        "config/*"
      ],
      "@/constants/*": [
        "constants/*"
      ],
      "@/models/*": [
        "models/*"
      ],
      "@/databases/*": [
        "databases/*"
      ],
      "@/middlewares/*": [
        "middlewares/*"
      ],
      "@/decorator/*": [
        "decorator/*"
      ]
    }
  }
}